{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "mocha": true,
    "cypress/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier", "cypress"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
